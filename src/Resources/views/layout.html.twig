{% extends "base.html.twig" %}

{%- block body %}
  <div class="expanded row">
    <div class="medium-3 large-2 medium-pull-9 large-pull-10 columns" data-sticky-container>
      <div class="sticky" data-sticky data-margin-top="0">
        {%- block sidebar %}
          {{ knp_menu_render('minimal_manager_main') }}
        {% endblock -%}
      </div>
    </div>
    <div class="small-12 medium-9 large-10 medium-push-3 large-push-2 columns">
      {%- if module.title is defined -%}
        <h3>{{module.title}}</h3>
      {%- else -%}
        <h3>{{"Manager"|trans}}</h3>
      {%- endif -%}
      {%- if module.description is defined -%}<p class="description">{{module.description}}</p>{%- endif -%}
      {%- block frame %}{%- block content %}{% endblock -%}{% endblock -%}
    </div>
  </div>
{% endblock -%}
